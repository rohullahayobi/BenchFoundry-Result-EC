0:CREATE TABLE `WAREHOUSE` (W_ID SMALLINT UNSIGNED UNIQUE, W_NAME VARCHAR(10), W_STREET_1 VARCHAR(20), W_STREET_2 VARCHAR(20), W_CITY VARCHAR(20), W_STATE CHAR(2), W_ZIP CHAR(9), W_TAX DECIMAL(4, 4), W_YTD DECIMAL(12, 2), PRIMARY KEY(W_ID));
1:CREATE TABLE `DISTRICT` (D_ID TINYINT UNSIGNED UNIQUE, W_ID SMALLINT UNSIGNED , D_NAME VARCHAR(10), D_STREET_1 VARCHAR(20), D_STREET_2 VARCHAR(20), D_CITY VARCHAR(20), D_STATE CHAR(2), D_ZIP CHAR(9), D_TAX DECIMAL(4, 4), D_YTD DECIMAL(12, 2), D_NEXT_O_ID MEDIUMINT UNSIGNED, PRIMARY KEY(D_ID));
2:CREATE TABLE `CUSTOMER` (C_ID MEDIUMINT UNSIGNED UNIQUE, D_ID TINYINT UNSIGNED , W_ID SMALLINT UNSIGNED , C_FIRST VARCHAR(16), C_MIDDLE CHAR(2), C_LAST VARCHAR(16), C_STREET_1 VARCHAR(20), C_STREET_2 VARCHAR(20), C_CITY_1 VARCHAR(20), C_STATE CHAR(2), C_ZIP CHAR(9), C_PHONE CHAR(16), C_SINCE DATETIME, C_CREDIT CHAR(2), C_CREDIT_LIM DECIMAL(12, 2), C_DISCOUNT DECIMAL(4, 4), C_BALANCE DECIMAL(12, 2), C_YTD_PAYMENT DECIMAL(4, 0), C_DELIVERY_CNT DECIMAL(4, 0), C_DATA VARCHAR(500), PRIMARY KEY(C_ID));
3:CREATE TABLE `ORDER` (O_ID MEDIUMINT UNSIGNED UNIQUE, O_D_ID TINYINT UNSIGNED, O_W_ID SMALLINT UNSIGNED, O_C_ID MEDIUMINT UNSIGNED, O_ENTRY_D DATETIME, O_CARRIER_ID DECIMAL(1, 0), O_OL_CNT DECIMAL(2, 0), O_ALL_LOCAL DECIMAL(1, 0), PRIMARY KEY(O_ID));
4:CREATE TABLE `NEW-ORDER` (NO_ID MEDIUMINT UNSIGNED UNIQUE, D_ID TINYINT UNSIGNED , W_ID SMALLINT UNSIGNED , NO_DATA CHAR(8), PRIMARY KEY(NO_ID));
5:CREATE TABLE `ITEM` (I_ID MEDIUMINT UNSIGNED UNIQUE, I_NAME VARCHAR(24), I_PRICE DECIMAL(5, 2), I_DATA VARCHAR(50), PRIMARY KEY(I_ID));
6:CREATE TABLE `STOCK` (S_ID MEDIUMINT UNSIGNED UNIQUE, I_ID MEDIUMINT UNSIGNED , W_ID SMALLINT UNSIGNED , S_QUANTITY DECIMAL(4, 0), S_DIST_01 CHAR(24), S_DIST_02 CHAR(24), S_DIST_03 CHAR(24), S_DIST_04 CHAR(24), S_DIST_05 CHAR(24), S_DIST_06 CHAR(24), S_DIST_07 CHAR(24), S_DIST_08 CHAR(24), S_DIST_09 CHAR(24), S_DIST_10 CHAR(24), S_YTD DECIMAL(8, 0), S_ORDER_CNT DECIMAL(4, 0), S_REMOTE_CNT DECIMAL(4, 0), S_DATA VARCHAR(50), PRIMARY KEY(S_ID));
7:CREATE TABLE `ORDER-LINE` (OL_NUMBER MEDIUMINT UNSIGNED UNIQUE, O_ID MEDIUMINT UNSIGNED , D_ID TINYINT UNSIGNED , W_ID SMALLINT UNSIGNED , I_ID MEDIUMINT UNSIGNED , OL_SUPPLY_W_ID SMALLINT UNSIGNED, OL_DELIVERY_D DATETIME, OL_QUANTITY DECIMAL(2, 0), OL_AMOUNT DECIMAL(6, 2), OL_DIST_INFO CHAR(24), PRIMARY KEY(OL_NUMBER));
8:CREATE TABLE `HISTORY` (H_ID MEDIUMINT UNSIGNED UNIQUE, H_DATE DATETIME, H_AMOUNT DECIMAL(6, 2), H_DATA VARCHAR(24), PRIMARY KEY(H_ID));
